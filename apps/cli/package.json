{
  "name": "@upft/cli",
  "version": "0.0.3",
  "type": "module",
  "description": "CLI for UPFT design token processing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unpunnyfuns/tokens.git",
    "directory": "apps/cli"
  },
  "license": "MIT",
  "author": "unpunnyfuns <unpunnyfuns@gmail.com>",
  "bin": {
    "upft": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@upft/analysis": "workspace:*",
    "@upft/ast": "workspace:*",
    "@upft/bundler": "workspace:*",
    "@upft/foundation": "workspace:*",
    "@upft/linter": "workspace:*",
    "@upft/loader": "workspace:*",
    "@upft/schemas": "workspace:*",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "esbuild": "^0.25.9",
    "execa": "^9.6.0",
    "picocolors": "^1.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "vite build --config vite.config.bundle.ts && chmod +x dist/cli.js",
    "check": "biome check --write .",
    "clean": "rm -rf dist",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config ../../vitest.config.ts",
    "test:e2e": "vitest run --config ../../vitest.e2e.config.ts",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/cli.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "design-tokens",
    "dtcg",
    "command-line"
  ],
  "dependencies": {
    "json5": "^2.2.3"
  }
}
