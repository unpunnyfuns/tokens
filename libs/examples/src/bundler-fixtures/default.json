{
  "color": {
    "primary": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [
          0,
          0.498,
          1
        ],
        "alpha": 1
      }
    },
    "secondary": {
      "$type": "color",
      "$value": {
        "colorSpace": "srgb",
        "components": [
          0.498,
          0,
          1
        ],
        "alpha": 1
      }
    },
    "neutral": {
      "100": {
        "$type": "color",
        "$value": {
          "colorSpace": "srgb",
          "components": [
            0.961,
            0.961,
            0.961
          ],
          "alpha": 1
        }
      },
      "900": {
        "$type": "color",
        "$value": {
          "colorSpace": "srgb",
          "components": [
            0.129,
            0.129,
            0.129
          ],
          "alpha": 1
        }
      }
    }
  },
  "typography": {
    "fontFamily": {
      "sans": {
        "$type": "fontFamily",
        "$value": [
          "Inter",
          "system-ui",
          "sans-serif"
        ]
      },
      "mono": {
        "$type": "fontFamily",
        "$value": [
          "Fira Code",
          "monospace"
        ]
      }
    },
    "fontSize": {
      "xs": {
        "$type": "dimension",
        "$value": {
          "value": 12,
          "unit": "px"
        }
      },
      "sm": {
        "$type": "dimension",
        "$value": {
          "value": 14,
          "unit": "px"
        }
      },
      "base": {
        "$type": "dimension",
        "$value": {
          "value": 16,
          "unit": "px"
        }
      },
      "lg": {
        "$type": "dimension",
        "$value": {
          "value": 18,
          "unit": "px"
        }
      },
      "xl": {
        "$type": "dimension",
        "$value": {
          "value": 20,
          "unit": "px"
        }
      }
    },
    "fontWeight": {
      "regular": {
        "$type": "number",
        "$value": 400
      },
      "medium": {
        "$type": "number",
        "$value": 500
      },
      "bold": {
        "$type": "number",
        "$value": 700
      }
    }
  }
}